#include <stdio.h>

void plantilla(char A[3][3],int p){
   
    for (int i = 0; i < 3; i++){
        for(int j = 0; j < 3; j++){
            p++;
            A[i][j]= p+'0';
            printf("  %d",p);
        }
        printf("\n");
    }
    printf("\n");
    printf("\n");
   
    triqui(A,p);
   
}

void triqui(char A[3][3],int p,char t)
{
    int r=0;
    int pox[8];
    int j=0;
    int s=0;
        while(r != 1){

            printf("Digite el dato: ");
